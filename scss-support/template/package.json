{
  "name": "vue-mini-scss-template",
  "version": "1.0.0",
  "description": "Vue Mini template with SCSS support",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node build.js",
    "build": "cross-env NODE_ENV=production node build.js",
    "lint": "eslint . --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss,less}\" --fix",
    "prettier": "prettier --write \"**/*.{js,ts,json,md,wxml}\"",
    "test": "vitest run",
    "prepare": "husky install"
  },
  "dependencies": {
