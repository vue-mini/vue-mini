    "posthtml": "^0.16.6",
    "prettier": "^3.4.2",
    "prettier-plugin-packagejson": "^2.5.6",
    "rollup": "^4.28.0",
    "sass": "^1.91.0",
    "serve": "^14.2.4",
    "stylelint": "^16.11.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-prettier": "^5.0.2",
    "terser": "^5.36.0",
