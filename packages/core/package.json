{
  "name": "@vue-mini/core",
  "version": "1.2.1",
  "description": "基于 Vue 3 的小程序框架。简单，强大，高性能。 ",
  "main": "dist/vue-mini.cjs.js",
  "module": "dist/vue-mini.esm-bundler.js",
  "types": "dist/vue-mini.d.ts",
  "repository": "https://github.com/vue-mini/vue-mini.git",
  "homepage": "https://vuemini.org",
  "author": "Yang Mingshan <y.mingshan3@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/vue-mini.d.ts",
      "import": "./dist/vue-mini.esm-bundler.js",
      "require": "./dist/vue-mini.cjs.js",
      "default": "./dist/vue-mini.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "vue",
    "mini",
    "wechat",
    "weixin",
    "miniprogram",
    "mp",
    "微信",
    "小程序"
  ],
  "dependencies": {
    "@vue/reactivity": "3.5.13",
    "@vue-mini/miniprogram-api-typings": "workspace:*"
  }
}
